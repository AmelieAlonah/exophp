INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (4, 'Zombie', 'Sed si ille hac tam eximia fortuna propter utilitatem rei publicae frui non properat, ut omnia illa conficiat, quid ego, senator, facere debeo, quem, etiamsi ille aliud vellet, rei publicae consulere oporteret?', 40, 2);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (2, 'Pink Lady', 'Nunc vero inanes flatus quorundam vile esse quicquid extra urbis pomerium nascitur aestimant praeter orbos et caelibes, nec credi potest qua obsequiorum diversitate coluntur homines sine liberis Romae.', 20, 2);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (3, 'Panda', 'Homines enim eruditos et sobrios ut infaustos et inutiles vitant, eo quoque accedente quod et nomenclatores adsueti haec et talia venditare, mercede accepta lucris quosdam et prandiis inserunt subditicios ignobiles et obscuros.', 50, 2);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (1, 'Kissing', 'Proinde concepta rabie saeviore, quam desperatio incendebat et fames, amplificatis viribus ardore incohibili in excidium urbium matris Seleuciae efferebantur, quam comes tuebatur Castricius tresque legiones bellicis sudoribus induratae.', 40, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (5, 'Minion', 'Ut enim benefici liberalesque sumus, non ut exigamus gratiam (neque enim beneficium faeneramur sed natura propensi ad liberalitatem sumus), sic amicitiam non spe mercedis adducti sed quod omnis eius fructus in ipso amore inest, expetendam putamus.', 44, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (6, 'Père Noël', 'Tempore quo primis auspiciis in mundanum fulgorem surgeret victura dum erunt homines Roma, ut augeretur sublimibus incrementis, foedere pacis aeternae Virtus convenit atque Fortuna plerumque dissidentes, quarum si altera defuisset, ad perfectam non venerat summitatem.', 36, 1);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (7, 'Sleepy', 'Vita est illis semper in fuga uxoresque mercenariae conductae ad tempus ex pacto atque, ut sit species matrimonii, dotis nomine futura coniunx hastam et tabernaculum offert marito, post statum diem si id elegerit discessura, et incredibile est quo ardore apud eos in venerem uterque solvitur sexus.', 40, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (8, 'Bear', 'Fieri, inquam, Triari, nullo pacto potest, ut non dicas, quid non probes eius, a quo dissentias. quid enim me prohiberet Epicureum esse, si probarem, quae ille diceret? cum praesertim illa perdiscere ludus esset.', 46, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (9, 'Pantufa', 'Quam ob rem dissentientium inter se reprehensiones non sunt vituperandae, maledicta, contumeliae, tum iracundiae, contentiones concertationesque in disputando pertinaces indignae philosophia mihi videri solent.', 48, 1);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (10, 'Jack', 'Quam ob rem id primum videamus, si placet, quatenus amor in amicitia progredi debeat. Numne, si Coriolanus habuit amicos, ferre contra patriam arma illi cum Coriolano debuerunt? num Vecellinum amici regnum adpetentem, num Maelium debuerunt iuvare?', 50, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (11, 'Teeturtle', 'Nec minus feminae quoque calamitatum participes fuere similium. nam ex hoc quoque sexu peremptae sunt originis altae conplures, adulteriorum flagitiis obnoxiae vel stuprorum. inter quas notiores fuere Claritas et Flaviana, quarum altera cum duceretur ad mortem.', 50, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (12, 'Pikachu', 'Claritas et Flaviana, quarum altera cum duceretur ad mortem, indumento, quo vestita erat, abrepto, ne velemen quidem secreto membrorum sufficiens retinere permissa est. ideoque carnifex nefas admisisse convictus inmane, vivus exustus est.',  50, 1);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (13, 'Unicorn', 'Haec igitur Epicuri non probo, inquam. De cetero vellem equidem aut ipse doctrinis fuisset instructior est enim, quod tibi ita videri necesse est, non satis politus iis artibus, quas qui tenent, eruditi appellantur aut ne deterruisset alios a studiis. quamquam te quidem video minime esse deterritum.', 50, 1);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (14, 'Shark', 'Intellectum est enim mihi quidem in multis, et maxime in me ipso, sed paulo ante in omnibus, cum M. Marcellum senatui reique publicae concessisti, commemoratis praesertim offensionibus, te auctoritatem huius ordinis dignitatemque rei publicae tuis vel doloribus vel suspicionibus anteferre', 40, 3);
INSERT INTO `product` (`id`, `name`, `description`, `price`, `brand_id`) VALUES (15, 'Eagles', 'Ille quidem fructum omnis ante actae vitae hodierno die maximum cepit, cum summo consensu senatus, tum iudicio tuo gravissimo et maximo. Ex quo profecto intellegis quanta in dato beneficio sit laus, cum in accepto sit tanta gloria.', 34, 1);
